{% extends 'base.html.twig' %}


{% block title %}{% trans %}patent.title{% endtrans %}{% endblock %}
{% block meta %}
    <meta name="description" content="{% trans %}patent.meta.description{% endtrans %}">
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('patent') }}
{% endblock %}

{% block body %}
    <main>
        {{ include('frontend/patent/_hero.html.twig') }}

        {{ include('frontend/patent/_content.html.twig') }}

    </main>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('patent') }}
{% endblock %}
