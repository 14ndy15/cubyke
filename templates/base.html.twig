<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>

        {{ include('frontend/global/_meta.html.twig') }}

        <title>{% block title %}{% endblock %} | Cubyke</title>
        {% block meta %}{% endblock %}

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>

        {{ include('frontend/global/_header.html.twig') }}

        {% block body %}{% endblock %}

        {{ include('frontend/global/_footer.html.twig') }}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
